{
  "name": "rss-resources-download",
  "version": "0.1.0",
  "description": "基于 rss 和 you-get 的视频、音频、图片下载器",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js,.cjs,.mjs",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsup": "cross-env NODE_ENV=development tsup --watch",
    "start": "node ./dist/index.mjs",
    "release": "semantic-release",
    "commit": "cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.4.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-cmyr-config": "^2.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-cmyr": "^1.1.1",
    "debug": "^4.3.6",
    "eslint": "^8.57.0",
    "eslint-config-cmyr": "^1.1.31",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "rimraf": "^6.0.1",
    "semantic-release": "^23.0.6",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-cmyr"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "tsup",
    "typescript",
    "template"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/rss-resources-download#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/rss-resources-download.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/rss-resources-download/issues"
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "cron": "^3.1.7",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "isomorphic-unfetch": "^4.0.2",
    "lodash-es": "^4.17.21",
    "log4js": "^6.9.1",
    "p-limit": "^6.1.0",
    "p-queue": "^8.0.1",
    "rss-parser": "^3.13.0",
    "tslib": "^2.6.3",
    "yaml": "^2.5.0",
    "zx": "^8.1.4"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0"
    }
  },
  "type": "module"
}