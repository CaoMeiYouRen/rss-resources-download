{
  "name": "rss-resources-download",
  "version": "0.1.0",
  "description": "基于 rss 和 you-get 的视频、音频、图片下载器",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js,.cjs,.mjs",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsup": "cross-env NODE_ENV=development tsup --watch",
    "start": "node ./dist/index.mjs",
    "start:tsx": "tsx src/index.ts",
    "release": "semantic-release",
    "commit": "cz",
    "prepare": "husky install",
    "start:pm2": "pm2 start ./pm2.json",
    "stop:pm2": "pm2 stop ./pm2.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@swc/core": "^1.15.1",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^24.10.0",
    "@vercel/nft": "^0.30.3",
    "commitizen": "^4.3.1",
    "commitlint-config-cmyr": "^1.0.0",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog-cmyr": "^2.0.0",
    "debug": "^4.4.3",
    "eslint": "^9.39.1",
    "eslint-config-cmyr": "^2.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "rimraf": "^6.1.0",
    "semantic-release": "^25.0.2",
    "semantic-release-cmyr-config": "^1.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-cmyr"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "tsup",
    "typescript",
    "template"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/rss-resources-download#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/rss-resources-download.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/rss-resources-download/issues"
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "better-bytes": "^1.2.4",
    "cron": "^4.3.4",
    "dayjs": "^1.11.12",
    "dotenv": "^17.2.3",
    "file-type": "^21.0.0",
    "fs-extra": "^11.3.2",
    "isomorphic-unfetch": "^4.0.2",
    "lodash-es": "^4.17.23",
    "log4js": "^6.9.1",
    "mime-types": "^2.1.35",
    "p-limit": "^7.2.0",
    "p-queue": "^9.1.0",
    "push-all-in-one": "^4.5.0",
    "reflect-metadata": "^0.2.2",
    "rss-parser": "^3.13.0",
    "smol-toml": "^1.5.2",
    "sqlite3": "^5.1.7",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.27",
    "yaml": "^2.8.1",
    "zx": "^8.8.5"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0",
      "tar-fs": "^2.1.3",
      "esbuild": "^0.25.0",
      "js-yaml": "^4.1.1",
      "tmp": "^0.2.4",
      "glob": "^12.0.0",
      "node-gyp": "^12.1.0",
      "cacache": "^20.0.0",
      "nodemailer": "^7.0.11"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "sqlite3"
    ]
  },
  "type": "module"
}